{
  "name": "ludora",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "start": "NODE_ENV=development expo start --port 8086",
    "start:doppler": "NODE_ENV=development node tools/update-ip/update-ip.js && cross-env DOPPLER_TOKEN=$(grep DOPPLER_TOKEN .env.development | cut -d '=' -f2) doppler run -- expo start --port 8086",
    "generate-icons": "node tools/svg-2-obj/index.js",
    "update-ip": "node tools/update-ip/update-ip.js",
    "android": "NODE_ENV=development expo run:android --port 8086",
    "android:doppler": "cross-env DOPPLER_TOKEN=$(grep DOPPLER_TOKEN .env.development | cut -d '=' -f2) doppler run -- expo run:android",
    "ios": "NODE_ENV=development expo run:ios --port 8086",
    "ios:doppler": "cross-env DOPPLER_TOKEN=$(grep DOPPLER_TOKEN .env.development | cut -d '=' -f2) doppler run -- expo run:ios",
    "build:android:dev:local": "eas build --platform android --profile development --local",
    "build:ios:dev:local": "eas build --platform ios --profile development --local",
    "doctor": "bunx expo-doctor",
    "install:check": "bunx expo install --check",
    "web": "expo start --web"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.69.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "expo": "~52.0.27",
    "expo-constants": "~17.0.3",
    "expo-image": "^2.0.7",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.11",
    "expo-secure-store": "^14.0.1",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "formik": "^2.4.6",
    "ky": "^1.7.5",
    "nativewind": "^4.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-keyboard-controller": "^1.16.8",
    "react-native-reanimated": "3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^3.4.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "cross-env": "^7.0.3",
    "jsdom": "^26.0.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
