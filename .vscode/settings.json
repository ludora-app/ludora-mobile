  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "eslint.validate": ["typescript", "javascript", "typescriptreact", "javascriptreact"],

  "eslint.options": {
    "extensions": [".ts", ".js", ".tsx", ".jsx"]
