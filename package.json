{
  "name": "ludora",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "start": "node tools/update-ip/update-ip.js && bun inject:env expo start --port 8086",
    "start:clear": "bun inject:env expo start --port 8086 --clear",
    "generate:icons": "node tools/svg-2-obj/index.js",
    "inject:env": "chmod +x tools/vault/inject-env.sh && bash tools/vault/inject-env.sh",
    "update:ip": "node tools/update-ip/update-ip.js",
    "android": "bun inject:env expo run:android",
    "ios": "bun inject:env expo run:ios",
    "build:android:dev:local": "bun inject:env eas build --platform android --profile development --local",
    "build:android:preview": "bun inject:env eas build --platform android --profile preview",
    "build:ios:dev:local": "bun inject:env eas build --platform ios --profile development --local",
    "doctor": "bunx expo-doctor",
    "install:check": "bunx expo install --check",
    "install:fix": "bunx expo install --fix",
    "web": "expo start --web",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "ts:check": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky",
    "check:all": "bun run lint && bun run ts:check",
    "generate:translations:local": "bun inject:env node tools/tolgee-translation/index.mjs",
    "download:swagger": "bun inject:env node tools/generate-api/download-swagger.mjs",
    "generate:api": "bun inject:env node tools/generate-api/download-swagger.mjs && bun inject:env node tools/generate-api/orval.mjs && bun inject:env bunx orval@next && node tools/generate-api/remove-controller.mjs && node tools/generate-api/fix-rq-v5.mjs"
  },
  "dependencies": {
    "@chillui/ui": "^0.0.0",
    "@expo-google-fonts/nunito-sans": "^0.4.2",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@hookform/resolvers": "^5.0.1",
    "@legendapp/list": "^2.0.16",
    "@mustapha-ghlissi/react-native-accordion": "^1.1.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^5.69.3",
    "@tolgee/format-icu": "^6.2.7",
    "@tolgee/react": "^6.2.7",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "expo": "~54.0.29",
    "expo-checkbox": "~5.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.10",
    "expo-location": "~19.0.8",
    "expo-plausible": "^0.1.1",
    "expo-router": "~6.0.19",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "ky": "^1.7.5",
    "nativewind": "^4.0.1",
    "radash": "^12.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.56.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-native-webview": "13.15.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
